{
  "name": "@tduniec/backstage-timesaver-plugin",
  "version": "1.0.2",
  "description": "This plugin provides an implementation of charts and statistics related to your time savings that are coming from usage of your templates.",
  "private": true,
  "engines": {
    "node": "18 || 20"
  },
  "workspaces": {
    "packages": [
      "plugins/*"
    ]
  },
  "repository": "https://github.com/tduniec/backstage-timesaver-plugin.git",
  "author": "tduniec <tomekduniec@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "build:all": "backstage-cli repo build --all",
    "clean": "backstage-cli repo clean",
    "tsc": "tsc",
    "tsc:full": "tsc --skipLibCheck false --incremental false",
    "test:all": "backstage-cli repo test --coverage",
    "fix": "backstage-cli repo fix",
    "lint": "backstage-cli repo lint --since origin/main",
    "lint:all": "backstage-cli repo lint",
    "prettier": "prettier --ignore-path .gitignore",
    "prettier:check-all": "prettier --ignore-path .gitignore --check .",
    "prettier:fix-all": "prettier --ignore-path .gitignore --write .",
    "test": "jest"
  },
  "resolutions": {
    "@types/react": "^18",
    "@types/react-dom": "^18"
  },
  "devDependencies": {
    "@backstage/cli": "^0.26.3",
    "@backstage/e2e-test-utils": "^0.1.1",
    "@changesets/cli": "^2.14.0",
    "@emotion/styled": "^11.11.5",
    "@material-ui/styles": "^4.11.5",
    "@material-ui/types": "^5.1.0",
    "@octokit/rest": "^19.0.3",
    "@playwright/test": "^1.32.3",
    "@spotify/eslint-config-typescript": "^15.0.0",
    "@spotify/eslint-plugin": "^14.1.3",
    "@spotify/prettier-config": "^14.0.0",
    "@types/aria-query": "^5.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.17.8",
    "@types/react": "^18.3.1",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.18.5",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "better-sqlite3": "^9.5.0",
    "command-exists": "^1.2.9",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.0",
    "csstype": "^3.1.3",
    "eslint": "^8.6.0",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-testing-library": "^6.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "fs-extra": "10.1.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lint-staged": "^15.0.0",
    "minimist": "^1.2.5",
    "node-gyp": "^10.0.0",
    "prettier": "^2.2.1",
    "semver": "^7.5.3",
    "shx": "^0.3.2",
    "ts-node": "^10.4.0",
    "typescript": "~5.3.0"
  },
  "prettier": "@spotify/prettier-config",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.md": [
      "node ./scripts/check-docs-quality"
    ],
    "{plugins,packages}/*/catalog-info.yaml": [
      "yarn backstage-repo-tools generate-catalog-info --ci"
    ],
    "{.github/CODEOWNERS,package.json}": [
      "yarn backstage-repo-tools generate-catalog-info",
      "git add */catalog-info.yaml"
    ],
    "./yarn.lock": [
      "node ./scripts/verify-lockfile-duplicates --fix"
    ],
    "*/yarn.lock": [
      "node ./scripts/verify-lockfile-duplicates --fix"
    ]
  },
  "packageManager": "yarn@3.8.1"
}
